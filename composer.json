{
    "name":         "takeatea/tea-theme-options",
    "type":         "wordpress-plugin",
    "description":  "Simple, easy to use and fully integrated Theme Options for Wordpress, made by Take a tea.",
    "keywords":     [
        "wordpress",
        "plugin",
        "teathemeoptions",
        "themeoptions",
        "tea",
        "tto",
        "teato",
        "theme",
        "options"
    ],
    "homepage":     "http://takeatea.com/",
    "license":      "MIT",
    "authors":      [
        {
            "name":     "Achraf Chouk",
            "homepage": "http://www.takeatea.com/"
        },
        {
            "name":     "Take a tea",
            "homepage": "http://www.takeatea.com/"
        }
    ],
    "require":      {
        "ruflin/Elastica":  "1.3.0.0"
    },
    "autoload":     {
        "files": [ "takeatea/tea-theme-options/src/TeaFunctions.php" ],
        "psr-4": { "Takeatea\\TeaThemeOptions\\": "src/" }
    },
    "extra":        {
        "branch-alias": {
            "dev-master": "1.0.x-dev"
        }
    },
    "minimum-stability": "dev"
}
